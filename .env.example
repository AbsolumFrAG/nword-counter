# Discord Bot Token
# Get your bot token from https://discord.com/developers/applications
DISCORD_BOT_TOKEN=your_bot_token_here

# PostgreSQL Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_NAME=nword_counter
DB_USER=postgres
